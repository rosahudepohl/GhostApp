<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.rosa.ghostapp" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Highscore"
            android:label="@string/title_activity_highscore" >
        </activity>
        <activity
            android:name=".SelectUser"
            android:label="@string/title_activity_select_user" >
        </activity>
        <activity
            android:name=".PlayGame"
            android:label="@string/title_activity_play_game" >
        </activity>
        <activity
            android:name=".GameEnded"
            android:label="@string/title_activity_game_ended" >
        </activity>
    </application>

</manifest>
